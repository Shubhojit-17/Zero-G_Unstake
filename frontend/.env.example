# Zero-G Unstake Frontend Environment Variables

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WC_PROJECT_ID=

# Optional: Override relayer API URL (uses /api by default)
# NEXT_PUBLIC_RELAYER_API=http://localhost:3001

# Relayer private key (for API route - DO NOT expose to frontend!)
RELAYER_PRIVATE_KEY=

# BSC Testnet RPC URL
BSC_TESTNET_RPC=https://bsc-testnet-rpc.publicnode.com

# Contract addresses (fill after deployment)
STAKING_VAULT_ADDRESS=
UNSTAKE_DELEGATE_ADDRESS=
